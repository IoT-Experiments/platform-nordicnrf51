{
  "build": {
    "core": "nRF5",
    "cpu": "cortex-m0",
    "extra_flags": "-DARDUINO_TINYBLE",
    "f_cpu": "16000000L",
    "ldscript": "nrf51_xxaa.ld",
    "mcu": "nrf51822",
    "variant": "TinyBLE"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "tools": {
      "cmsis-dap": {
        "onboard": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/cmsis-dap.cfg",
            "-f",
            "scripts/target/nrf51.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        }
      }
    }
  },
  "frameworks": [
    "arduino",
    "mbed"
  ],
  "name": "Seeed Tiny BLE",
  "upload": {
    "maximum_ram_size": 16384,
    "maximum_size": 262144,
    "protocol": "cmsis-dap",
    "protocols": [
      "cmsis-dap",
      "blackmagic",
      "jlink",
      "nrfjprog",
      "stlink",
      "mbed"
    ]
  },
  "url": "http://developer.mbed.org/platforms/Seeed-Tiny-BLE/",
  "vendor": "SeeedStudio"
}