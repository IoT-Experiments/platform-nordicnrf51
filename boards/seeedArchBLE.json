{
  "build": {
    "cpu": "cortex-m0", 
    "f_cpu": "16000000L", 
    "mcu": "nrf51822"
  }, 
  "connectivity": [
    "bluetooth"
  ], 
  "debug": {
    "tools": {
      "cmsis-dap": {
        "onboard": true, 
        "server": {
          "arguments": [
            "-f", 
            "scripts/interface/cmsis-dap.cfg", 
            "-f", 
            "scripts/target/nrf51.cfg"
          ], 
          "executable": "bin/openocd", 
          "package": "tool-openocd"
        }
      }
    }
  }, 
  "frameworks": [
    "mbed"
  ], 
  "name": "Seeed Arch BLE", 
  "upload": {
    "maximum_ram_size": 16384, 
    "maximum_size": 131072, 
    "protocol": "cmsis-dap", 
    "protocols": [
      "cmsis-dap", 
      "blackmagic", 
      "jlink", 
      "nrfjprog", 
      "stlink", 
      "mbed"
    ]
  }, 
  "url": "https://developer.mbed.org/platforms/Seeed-Arch-BLE/", 
  "vendor": "SeeedStudio"
}