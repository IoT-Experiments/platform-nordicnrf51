{
  "build": {
    "cpu": "cortex-m0",
    "f_cpu": "16000000L",
    "mcu": "nrf51822"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-f", "scripts/interface/cmsis-dap.cfg",
            "-f", "scripts/target/nrf51.cfg"
          ]
        },
        "onboard": true
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "JKSoft Wallbot BLE",
  "upload": {
    "maximum_ram_size": 16384,
    "maximum_size": 131072,
    "protocols": [
      "cmsis-dap",
      "mbed"
    ],
    "protocol": "cmsis-dap"
  },
  "url": "https://developer.mbed.org/platforms/JKSoft-Wallbot-BLE/",
  "vendor": "JKSoft"
}